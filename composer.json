{
  "name": "enl/swiftmailer-extras",
  "description": "Extra features for Swiftmailer - PHP mailing library",
  "keywords": ["swiftmailer", "psr3", "monolog", "runtime spool", "priority"],
  "authors": [{
    "name": "Alex Panshin",
    "email": "deadyaga@gmail.com",
    "role": "Developer"
  }],
  "require": {
    "php": "~5.6|~7.0",
    "swiftmailer/swiftmailer": "~5.4",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.4",
    "mockery/mockery": "~0.9"
  },
  "suggest": {
    "monolog/monolog": "to use PSR-3 logger adapter"
  },
  "autoload": {
    "psr-4": {
      "Enl\\Swiftmailer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wnl\\Swiftmailer\\Test\\": "tests/"
    }
  }
}
